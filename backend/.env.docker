# Object Storage (S3-compatible: RustFS for Docker)
S3_REGION=us-east-1
S3_ACCOUNT_ID=local
S3_BUCKET=ruxlog-docker
S3_ACCESS_KEY=GKffffaaaaaaaaaaaaaaaaaaaa
S3_SECRET_KEY=aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa
S3_PUBLIC_URL=http://127.0.0.1:9000
S3_ENDPOINT=http://rustfs:9000

# RustFS Configuration
RUSTFS_ACCESS_KEY=${S3_ACCESS_KEY}
RUSTFS_SECRET_KEY=${S3_SECRET_KEY}
RUSTFS_API_PORT=9000
RUSTFS_CONSOLE_PORT=9001

# Image Optimization
OPTIMIZE_ON_UPLOAD=true
OPTIMIZER_MAX_PIXELS=40000000
OPTIMIZER_KEEP_ORIGINAL=true
OPTIMIZER_WEBP_QUALITY_DEFAULT=80

# OpenTelemetry / Quickwit
ENABLE_QUICKWIT_OTEL=false
QUICKWIT_API_URL=http://localhost:7280
QUICKWIT_INGEST_URL=http://localhost:7280
QUICKWIT_LOGS_INDEX_ID=otel-logs-v0_7
QUICKWIT_TRACES_INDEX_ID=otel-traces-v0_7
QUICKWIT_METRICS_INDEX_ID=otel-metrics-v0_7
QUICKWIT_ACCESS_TOKEN=
OTEL_SERVICE_NAME=ruxlog-api
OTEL_SERVICE_VERSION=0.1.0
OTEL_BSP_MAX_QUEUE_SIZE=2048
OTEL_BSP_SCHEDULE_DELAY=5000
OTEL_BSP_MAX_EXPORT_BATCH_SIZE=512
OTEL_BSP_EXPORT_TIMEOUT=30000
OTEL_METRIC_EXPORT_INTERVAL=30000
OTEL_METRIC_EXPORT_TIMEOUT=30000
OTEL_BLRP_MAX_QUEUE_SIZE=2048
OTEL_BLRP_SCHEDULE_DELAY=1000
OTEL_BLRP_MAX_EXPORT_BATCH_SIZE=512
OTEL_BLRP_EXPORT_TIMEOUT=30000
OTEL_TRACES_SAMPLER_ARG=1.0
OTEL_EXPORTER_OTLP_HEADERS=

# Third-party integrations
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=

# Supabase
SUPABASE_URL=http://localhost:54321
